                {
                  "key": "project_id",
                  "value": "PROJECT_ID_HERE",
                  "description": "Optional - Filter by project ID"
                },
                {
                  "key": "service_id",
                  "value": "SERVICE_ID_HERE",
                  "description": "Optional - Filter by service ID"
                },
                {
                  "key": "startDate",
                  "value": "2024-01-01",
                  "description": "Optional - Filter logs from this date (YYYY-MM-DD)"
